{
    "$defs": {
        "CanConfigurationAction": {
            "description": "Action for configuring the CAN interface\n    ",
            "properties": {
                "channel": {
                    "description": "The CAN interface e.g. can0",
                    "title": "Channel",
                    "type": "string"
                },
                "state": {
                    "allOf": [
                        {
                            "$ref": "#/$defs/InterfaceState"
                        }
                    ],
                    "default": "UP",
                    "description": "The state of the CAN interface - UP/DOWN"
                },
                "bitrate": {
                    "default": 500000,
                    "description": "Bitrate",
                    "title": "Bitrate",
                    "type": "integer"
                },
                "sample_point": {
                    "default": 0.875,
                    "description": "Sample-point",
                    "title": "Sample Point",
                    "type": "number"
                },
                "cc_len8_dlc": {
                    "description": "cc-len8-dlc flag value",
                    "title": "Cc Len8 Dlc",
                    "type": "boolean"
                },
                "fd": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/CanFdOptions"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Set interface to support CAN-FD"
                },
                "action_type": {
                    "const": "con_conf",
                    "default": "con_conf",
                    "title": "Action Type"
                }
            },
            "required": [
                "channel",
                "cc_len8_dlc"
            ],
            "title": "CanConfigurationAction",
            "type": "object"
        },
        "CanFdOptions": {
            "properties": {
                "dbitrate": {
                    "default": 2000000,
                    "description": "The data bitrate",
                    "title": "Dbitrate",
                    "type": "integer"
                }
            },
            "title": "CanFdOptions",
            "type": "object"
        },
        "ConfigurationManager": {
            "properties": {
                "actions": {
                    "anyOf": [
                        {
                            "items": {
                                "anyOf": [
                                    {
                                        "$ref": "#/$defs/IpAddAction"
                                    },
                                    {
                                        "$ref": "#/$defs/IpRemoveAction"
                                    },
                                    {
                                        "$ref": "#/$defs/WifiConnectAction"
                                    },
                                    {
                                        "$ref": "#/$defs/CanConfigurationAction"
                                    },
                                    {
                                        "$ref": "#/$defs/EthInterfaceConfigurationAction"
                                    },
                                    {
                                        "$ref": "#/$defs/CreateVlanAction"
                                    }
                                ]
                            },
                            "type": "array"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "title": "Actions"
                }
            },
            "title": "ConfigurationManager",
            "type": "object"
        },
        "CreateVlanAction": {
            "description": "Action for creating a VLAN interface linked to an actual Eth interface\n    ",
            "properties": {
                "action_type": {
                    "const": "vlan_create",
                    "default": "vlan_create",
                    "title": "Action Type"
                },
                "if_name": {
                    "description": "The new vlan interface name",
                    "title": "If Name",
                    "type": "string"
                },
                "if_link": {
                    "description": "The physical interface to link to",
                    "title": "If Link",
                    "type": "string"
                },
                "vlan_id": {
                    "description": "The vlan ID",
                    "title": "Vlan Id",
                    "type": "integer"
                }
            },
            "required": [
                "if_name",
                "if_link",
                "vlan_id"
            ],
            "title": "CreateVlanAction",
            "type": "object"
        },
        "EthIfFlags": {
            "enum": [
                "IFF_UP",
                "IFF_BROADCAST",
                "IFF_DEBUG",
                "IFF_LOOPBACK",
                "IFF_POINTOPOINT",
                "IFF_NOTRAILERS",
                "IFF_RUNNING",
                "IFF_NOARP",
                "IFF_PROMISC",
                "IFF_ALLMULTI",
                "IFF_MASTER",
                "IFF_SLAVE",
                "IFF_MULTICAST",
                "IFF_PORTSEL",
                "IFF_AUTOMEDIA",
                "IFF_DYNAMIC",
                "IFF_LOWER_UP",
                "IFF_DORMANT",
                "IFF_ECHO"
            ],
            "type": "string"
        },
        "EthInterfaceConfigurationAction": {
            "description": "Action for configuring the Ethernet interface\n    ",
            "properties": {
                "interface": {
                    "description": "The Eth interface to be configured",
                    "title": "Interface",
                    "type": "string"
                },
                "mtu": {
                    "anyOf": [
                        {
                            "type": "integer"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "MTU (maximum transmission unit)",
                    "title": "Mtu"
                },
                "flags": {
                    "default": [
                        "IFF_BROADCAST",
                        "IFF_MULTICAST",
                        "IFF_UP",
                        "IFF_LOWER_UP",
                        "IFF_RUNNING"
                    ],
                    "description": "Flags to apply on the interface",
                    "items": {
                        "$ref": "#/$defs/EthIfFlags"
                    },
                    "title": "Flags",
                    "type": "array"
                },
                "state": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/InterfaceState"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Interface State to configure"
                },
                "action_type": {
                    "const": "eth_conf",
                    "default": "eth_conf",
                    "title": "Action Type"
                }
            },
            "required": [
                "interface"
            ],
            "title": "EthInterfaceConfigurationAction",
            "type": "object"
        },
        "InterfaceState": {
            "description": "Enum for the state of the Ethernet interface\n    ",
            "enum": [
                "UP",
                "DOWN",
                "UNKNOWN"
            ],
            "title": "InterfaceState",
            "type": "string"
        },
        "IpAddAction": {
            "description": "Action for adding an IP address to an ethernet interface\n    ",
            "properties": {
                "interface": {
                    "description": "The network interface for the IP to be configured",
                    "title": "Interface",
                    "type": "string"
                },
                "ip": {
                    "description": "The IP to configure, IPv4/IPv6",
                    "format": "ipvanyaddress",
                    "title": "Ip",
                    "type": "string"
                },
                "suffix": {
                    "description": "The subnet notation for this IP address",
                    "title": "Suffix",
                    "type": "integer"
                },
                "route": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/IpRoute"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Optional parameter for setting a route for the IP"
                },
                "action_type": {
                    "const": "add_ip",
                    "default": "add_ip",
                    "title": "Action Type"
                }
            },
            "required": [
                "interface",
                "ip",
                "suffix"
            ],
            "title": "IpAddAction",
            "type": "object"
        },
        "IpRemoveAction": {
            "description": "Action for removing an IP address to an ethernet interface\n    ",
            "properties": {
                "interface": {
                    "description": "The network interface for the IP to be configured",
                    "title": "Interface",
                    "type": "string"
                },
                "ip": {
                    "description": "The IP to configure, IPv4/IPv6",
                    "format": "ipvanyaddress",
                    "title": "Ip",
                    "type": "string"
                },
                "suffix": {
                    "description": "The subnet notation for this IP address",
                    "title": "Suffix",
                    "type": "integer"
                },
                "route": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/IpRoute"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Optional parameter for setting a route for the IP"
                },
                "action_type": {
                    "const": "del_ip",
                    "default": "del_ip",
                    "title": "Action Type"
                }
            },
            "required": [
                "interface",
                "ip",
                "suffix"
            ],
            "title": "IpRemoveAction",
            "type": "object"
        },
        "IpRoute": {
            "properties": {
                "gateway": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "Optional parameter the route gateway, none for default gateway",
                    "title": "Gateway"
                }
            },
            "title": "IpRoute",
            "type": "object"
        },
        "WifiConnectAction": {
            "description": "Action for connecting to a wifi network\n    ",
            "properties": {
                "action_type": {
                    "const": "wifi_connect",
                    "default": "wifi_connect",
                    "title": "Action Type"
                },
                "ssid": {
                    "description": "The SSID of the access point to connect to",
                    "title": "Ssid",
                    "type": "string"
                },
                "password": {
                    "description": "The pass phrase to use for connecting",
                    "title": "Password",
                    "type": "string"
                }
            },
            "required": [
                "ssid",
                "password"
            ],
            "title": "WifiConnectAction",
            "type": "object"
        }
    },
    "properties": {
        "cm": {
            "$ref": "#/$defs/ConfigurationManager"
        }
    },
    "required": [
        "cm"
    ],
    "title": "ConfigurationManagerRunnable",
    "type": "object"
}